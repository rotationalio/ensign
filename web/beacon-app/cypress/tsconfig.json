{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "noEmit": true,
    "target": "esnext",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "module": "esnext",
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "types": ["cypress", "node"],
  },
 "include": [
  "**/*.ts",
  ],
  "exclude": ["node_modules"]
}