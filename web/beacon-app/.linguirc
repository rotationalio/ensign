{
  "locales": [
    "en",
    "cs",
    "fr"
  ],
  "sourceLocale": "en",
  "catalogs": [
    {
      "path": "src/locales/{locale}/messages",
      "include": [
        "src"
      ]
    }
  ],
  "format": "po",
  "rootDir": ".",
  "extractBabelOptions": {
    "presets": [
      "@babel/preset-typescript"
    ]
  }
}
